{
    "name": "vitexsoftware/multiflexi",
    "description": "MultiFlexi web site",
    "type": "project",
    "require": {
        "php": "^8.0|^8.1|^8.2",
        "vitexsoftware/ease-twbootstrap5": "dev-main",
        "vitexsoftware/ease-html-widgets": "dev-main",
        "vitexsoftware/ease-fluentpdo": "dev-main",
        "datatables/datatables": "dev-master",
        "rcubitto/json-pretty": "dev-master",
        "vitexsoftware/ease-core-bricks": "dev-master"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Vítězslav Dvořák",
            "email": "info@vitexsoftware.cz"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "MultiFlexi\\": "src/MultiFlexi/",
            "MultiFlexi\\Ui\\": "src/MultiFlexi/Ui",
            "MultiFlexi\\Api\\": "src/MultiFlexi/Api"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\AbraFlexi\\": "vendor/spoje.net/abraflexi/testing/src/AbraFlexi/",
            "Test\\AbraFlexi\\MultiFlexi\\": "tests/AbraFlexi/MultiFlexi/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "phpstan/phpstan": "*",
        "friendsofphp/php-cs-fixer": "^3.61",
        "ergebnis/composer-normalize": "^2.43",
        "ergebnis/php-cs-fixer-config": "^6.34"
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        }
    }
}
